<div class="home col-xs-12 ">
    <div class="row">
        <table class="channels" border="1" >
            <tr>
                <td class="channelleft" style="width:220px;">
                    {{#if isAuthenticated}}
                    <h4>Channels<img style="margin-left=20px;" src="/images/help.png" onclick="javascript:location.href='/aboutchannels'">
                    <a title="Add Channel" href="/channel/new" style="padding-left:100px;"><b>+</b></a>
                    </h4>
                    {{else}}
                    <h4>Channels<img style="margin-left=20px;" src="/images/help.png" onclick="javascript:location.href='/aboutchannels'"></h4>
                    {{/if}}
                    {{#each channellist}}
                        <a href="/channels/{{id}}">{{statement}}</a><br/>
                    {{/each}}
                </td>
                <td class="channelright">
                    {{#each journallist}}
                        <a href="/journal/{{id}}"><img style="margin-right: 4px;" src="{{img}}">{{statement}}</a><br/>
                    {{/each}}
                </td>
            </tr>
        </table>
    </div>
</div>